{
  "name": "timer-compiler",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "fix": "pnpm run --parallel '/(lint:fix|format)/'",
    "format": "prettier --write --log-level=warn '**/!(*.min|*-lock).{js,mjs,cjs,ts,mts,cts,jsx,tsx,html,css,scss,sass,less,json,jsonc,yaml,yml,graphql,gql,md,mdx}'",
    "lint": "pnpm run --parallel '/lint:(?!.*fix).+$/'",
    "lint:fix": "pnpm run --parallel '/lint:.*:fix$/'",
    "lint:es": "eslint .",
    "lint:es:fix": "eslint . --fix",
    "lint:style": "stylelint '{src,app,pages}/**/*.{css,scss,sass,less}'",
    "lint:style:fix": "stylelint --fix '{src,app,pages}/**/*.{css,scss,sass,less}'",
    "preview": "vite preview",
    "preinstall": "pnpx typesync"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.469.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/eslint-plugin-react-refresh": "^0.4.0",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "babel-plugin-react-compiler": "19.0.0-beta-27714ef-20250124",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-compiler": "19.0.0-beta-27714ef-20250124",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.14.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "stylelint": "^16.13.2",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-standard": "^37.0.0",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.20.1",
    "vite": "^6.0.7",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
